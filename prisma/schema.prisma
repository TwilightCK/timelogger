datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Entry {
  id        Int      @id @default(autoincrement())
  entryId   String   @unique
  user      String
  start     DateTime
  end       DateTime
  duration  Int
  noteHash  String?
  txHash    String
  createdAt DateTime @default(now())
}
